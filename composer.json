{
    "name": "zaghadon/scaffolton",
    "type": "project",
    "description": "A quick Laravel Scaffold with Backpack Devtools Pro, BackupManager and PermissionManager.",
    "keywords": [
        "laravel",
        "scaffold",
        "backpack"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "backpack/backupmanager": "^3.0",
        "backpack/permissionmanager": "^6.0",
        "backpack/settings": "^3.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel-notification-channels/fcm": "~2.0",
        "laravel/framework": "^8.65",
        "laravel/jetstream": "^2.4",
        "laravel/sanctum": "^2.11",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.5",
        "livewire/livewire": "^2.5",
        "backpack/crud": "^5.0.0",
        "backpack/pro": "^1.0.0"
    },
    "require-dev": {
        "backpack/devtools": "^1.0",
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "repositories": {
        "backpack/devtools": {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com"
        },
        "0": {
            "type": "composer",
            "url": "https://repo.backpackforlaravel.com/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
